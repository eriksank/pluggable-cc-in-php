//These are examples of CAST conversions

// (1) int2string
a=(string)-100;
println('a='.a);

// (2) string2int
a=(int)"50";
println('a='.a);

// (4) int2float
a=(float)5;
println('a='.a);

// (5) float2int
a=(int)5.3;
println('a='.a);

// (6) float2string
a=(string)5.3;
println('a='.a);

// (7) string2float
a=(float)"5.3";
println('a='.a);

